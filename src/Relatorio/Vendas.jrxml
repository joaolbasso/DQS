	<query language="sql"><![CDATA[SELECT pagamento.data_pagamento,
	item_caixa.valor_item_caixa,
	pagamento.metodo_pagamento,
	item_venda.valor_unitario,
	item_venda.quantidade,
	venda.valor_venda,
	item_caixa.descricao_item_caixa,
	venda.id_venda
FROM item_caixa
	INNER JOIN pagamento ON 
	 item_caixa.id_pagamento = pagamento.id_pagamento 
	LEFT JOIN parcela ON 
	 pagamento.id_parcela = parcela.id_parcela 
	INNER JOIN venda ON 
	 parcela.id_venda = venda.id_venda 
	INNER JOIN itens_venda ON 
	 itens_venda.id_venda = venda.id_venda 
	INNER JOIN item_venda ON 
	 itens_venda.id_item_venda = item_venda.id_item_venda 
	 WHERE 
	 pagamento.data_pagamento BETWEEN '$P!{startDate}' AND '$P!{endDate}'
]]></query>

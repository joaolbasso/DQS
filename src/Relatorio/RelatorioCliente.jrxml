	<query language="sql"><![CDATA[SELECT venda.valor_venda,
	venda.data_venda,
	parcela.valor_parcela,
	pagamento.valor_pagamento,
	pagamento.data_pagamento,
	item_caixa.id_caixa
FROM item_caixa
	INNER JOIN pagamento ON 
	 item_caixa.id_pagamento = pagamento.id_pagamento 
	INNER JOIN parcela ON 
	 pagamento.id_parcela = parcela.id_parcela 
	INNER JOIN venda ON 
	 parcela.id_venda = venda.id_venda 
	INNER JOIN caixa ON 
	 item_caixa.id_caixa = caixa.id_caixa]]></query>
